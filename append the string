import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        String str1=s.nextLine();
        String str2=s.nextLine();
        StringBuffer sb=new StringBuffer(str1);
        sb.append(str2);
        System.out.print(sb);
    }
}
