import java.util.Scanner;
public class Solution {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int N = sc.nextInt();
        int[] arr = new int[N];

        for (int i = 0; i < N; i++) {
            arr[i] = sc.nextInt();
        }
        int K = sc.nextInt();
        if (K >= 0 && K < N) {
            System.out.println(arr[K]);
        } else {
            System.out.println("Invalid Index");
        }
        sc.close();
    }
}
