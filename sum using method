
import java.io.*;
import java.util.*;

public class Solution {

    // Method to return sum
    public static int sum(int a, int b) {
        return a + b;
    }

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        
        int a = sc.nextInt();
        int b = sc.nextInt();
        
        int result = sum(a, b);  // calling method
        
        System.out.println(result);
        
        sc.close();
    }
}
