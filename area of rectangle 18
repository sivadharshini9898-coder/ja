import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        if (sc.hasNext()) {
            long length = sc.nextLong();
            long breadth = sc.nextLong();
            
            long area = length * breadth;
            
            System.out.print(area);
        }
    }
}


