import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        if (sc.hasNextInt()) {
            int M = sc.nextInt();
            if (M >= 90) {
                System.out.println("A");
            } else if (M >= 75) {
                System.out.println("B");
            } else if (M >= 60) {
                System.out.println("C");
            } else {
                System.out.println("Fail");
            }
        }
        
        sc.close();
    }
}

