import java.util.*;

public class Solution {
    public static void main(String[] args) {
        Scanner s = new Scanner(System.in);

        int n = s.nextInt();
        int sum = 0;

        for (int i = 0; i < n; i++) {
            if (s.hasNextInt()) {
                sum += s.nextInt();
            }
        }

        System.out.println(sum);
        s.close();
    }
}
