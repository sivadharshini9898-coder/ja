import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int year;
        year=s.nextInt();
        if(year%4==0){
            System.out.print("Leap Year");
        }else{
            System.out.print("Not Leap Year");
           
        }
           
         
    }
}
