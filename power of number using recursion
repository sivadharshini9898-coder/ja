
import java.io.*;
import java.util.*;

public class Solution {
    static int number(int a,int b){
        int c=1;
        for(int i=1;i<=b;i++){
            c=c*a;
        }
        return c;
        }
    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int a=s.nextInt();
        int b=s.nextInt();
        int result=number(a,b);
        System.out.print(result);
     }
}
