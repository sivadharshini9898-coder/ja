
import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        String s = sc.nextLine();   // Original string
        String t = sc.nextLine();   // Replacement string
        int start = sc.nextInt();   // Start index
        int end = sc.nextInt();     // End index

        String result = s.substring(0, start) 
                       + t 
                       + s.substring(end);

        System.out.println(result);

        sc.close();
    }
}
