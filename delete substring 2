
import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        String s = sc.nextLine();   
        int start = sc.nextInt();   
        int end = sc.nextInt();    

        String result = s.substring(0, start) 
                       + s.substring(end);

        System.out.println(result);

        sc.close();
    }
}
