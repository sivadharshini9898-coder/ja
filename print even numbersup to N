import java.util.Scanner;

public class Solution {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int N = sc.nextInt();
        
        for(int i = 2; i <= N; i += 2) {
            System.out.print(i);
            if(i + 2 <= N) {
                System.out.print(" ");
            }
        }
    }
} 
